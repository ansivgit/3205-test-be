{
  "name": "3205-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon",
    "build": "./node_modules/.bin/esbuild ./src/app.ts --bundle --platform=node --outfile=./dist/index.js",
    "db:model:user": "sequelize-cli model:generate --name UsersDB --attributes login:string,password:string,age:integer,isDeleted:boolean",
    "db:model:group": "sequelize-cli model:generate --name GroupsDB --attributes name:string,permissions:Array",
    "db:model:userGroup": "sequelize-cli model:generate --name UsersGroupsDB --attributes userId:string,groupId:string",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansivgit/3205-test-be.git"
  },
  "author": "Anna ZAS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ansivgit/3205-test-be/issues"
  },
  "homepage": ".",
  "dependencies": {
    "dotenv": "^16.0.3",
    "esbuild": "0.17.15",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.22",
    "prettier": "2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
